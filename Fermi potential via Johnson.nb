Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"VF", "[", "r_", "]"}], ":=", 
   RowBox[{"Piecewise", "[", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{
        RowBox[{
         FractionBox["Z", 
          RowBox[{"A", " ", "c"}]], 
         RowBox[{"(", 
          RowBox[{
           FractionBox["3", "2"], "-", 
           FractionBox[
            SuperscriptBox["r", "2"], 
            RowBox[{"2", " ", 
